.com_list_box{
  display: flex;
  align-items: center;
  margin-top: 30rpx;
  background-color: #ffffff;
}
.list_box_left{
  flex: 2;

}
.list_box_right{
  flex: 3;
  display: flex;
  flex-direction: column;
  justify-content: space-around;

}
.box_left_img{
  width: 260rpx;
  height: 190rpx;
  border-radius: 20rpx;
}
.box_right_title{
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}
.box_right_brief{
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}
.box_right_tag_box{
  display: flex;
  align-items: center;
  margin-top: 15rpx;

}
.right_tag_icon{ 
  padding: 0 10rpx;
  height: 32rpx;
  background: #eaf1fd;
  border-radius: 16rpx;
  margin-right: 10rpx;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #3176EA;
  text-align: center;
  line-height: 32rpx;
}
.box_right_foot_price{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 48rpx;
  margin-top: 30rpx;
}
.price_num{
  font-size: 26rpx;
  font-family: Source Han Sans CN;
  font-weight: bold;
  color: #333333;
}
